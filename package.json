{
  "name": "node-file-rev",
  "version": "0.0.1",
  "description": "Simple node cli tool to revision files and write them to a rev-manifest.json",
  "main": "cli.js",
  "dependencies": {
    "chalk": "^2.4.1",
    "edit-json-file": "^1.2.0",
    "hasha": "^3.0.0",
    "meow": "^5.0.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {},
  "scripts": {
    "pretest": "npm run clean && npm run prepare",
    "test": "node-file-rev tests/assets/test.css,tests/assets/other.file.css --manifest=tests/assets/rev.json",
    "clean": "rm -r tests/assets/*",
    "prepare": "echo 'hello{ color: red; }' >> tests/assets/test.css && echo 'div{ border: 1px solid red; }' >> tests/assets/other.file.css"
  },
  "bin": {
    "node-file-rev": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasoppermann/node-file-rev.git"
  },
  "keywords": [
    "node file rev",
    "file rev"
  ],
  "author": "Lukas Oppermann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukasoppermann/node-file-rev/issues"
  },
  "homepage": "https://github.com/lukasoppermann/node-file-rev#readme"
}
